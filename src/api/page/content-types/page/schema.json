{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": true,
      "regex": "^[a-zA-Z0-9_-]+$"
    },
    "landing_img": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "landing_description": {
      "type": "text"
    },
    "meta_description": {
      "type": "text"
    },
    "meta_title": {
      "type": "string"
    },
    "main_image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "requirements": {
      "type": "richtext"
    },
    "helpful_links": {
      "type": "richtext"
    },
    "source": {
      "type": "richtext"
    },
    "content": {
      "type": "richtext"
    }
  }
}
